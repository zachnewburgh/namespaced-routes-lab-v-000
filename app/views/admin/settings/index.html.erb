<%= form_for @preference, url: admin_access_path(@preference) do |f| %>

  <%= f.label :song_sort_order %><br>
  Ascending: <%= f.radio_button :song_sort_order, "DESC" %><br>
  Descending: <%= f.radio_button :song_sort_order, "ASC" %><br>
  
  <%= f.label :artist_sort_order %><br>
  Ascending: <%= f.radio_button :artist_sort_order, "DESC" %><br>
  Descending: <%= f.radio_button :artist_sort_order, "ASC" %><br>
  
  <%= f.submit %>

<% end %>